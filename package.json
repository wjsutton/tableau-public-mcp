{
  "name": "@tableau-public/mcp-server",
  "version": "1.0.0",
  "description": "MCP server for Tableau Public APIs - enable AI applications to interact with Tableau Public content programmatically",
  "type": "module",
  "main": "./build/index.js",
  "bin": {
    "tableau-public-mcp-server": "./build/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src --ext .ts",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "mcp",
    "tableau",
    "tableau-public",
    "model-context-protocol",
    "ai",
    "visualization",
    "data-viz"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.25.1",
    "adm-zip": "^0.5.16",
    "axios": "^1.13.2",
    "exceljs": "^4.4.0",
    "fast-xml-parser": "^4.5.1",
    "papaparse": "^5.5.3",
    "sharp": "^0.34.5",
    "ts-results-es": "^4.2.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/adm-zip": "^0.5.7",
    "@types/node": "^20.19.27",
    "@types/papaparse": "^5.5.2",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitest/coverage-v8": "^4.0.16",
    "eslint": "^8.57.1",
    "typescript": "^5.7.2",
    "vitest": "^4.0.16"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
